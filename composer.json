{
    "name": "chowjiawei/tiktok-pay",
    "type": "library",
    "description": "tiktok 抖音-新交易系统 -泛知识 、非泛知识 支付系统",
    "keywords": ["laravel", "tiktok", "pay", "tiktok-pay"],
    "homepage": "https://github.com/chowjiawei/tiktok-pay",
    "license": "MIT",
    "authors": [
        {
            "name": "chowjiawei",
            "email": "1084186870@qq.com"
        }
    ],
    "require": {
        "php": "^7.0|^8.0",
        "guzzlehttp/guzzle": "^6.3|^7.0",
        "laravel/framework": "^6.0|^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "Chowjiawei\\Helpers\\": "src/"
        }
    },
    "scripts": {
        "lint": "phpcs --standard=PSR1,PSR12,./ruleset.xml  app/ routes/ config/",
        "lint-fix": "phpcbf --standard=PSR1,PSR12   src/"
    },
    "extra": {
        "laravel": {
            "providers": [
                "TikTokPayServiceProvider"
            ]
        }
    },
}
